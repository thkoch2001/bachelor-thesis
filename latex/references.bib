@MASTERSTHESIS{Schneider2010,
  author = {Schneider, Andreas},
  title = {Vergleich und Beurteilung von Java Frameworks für Web Services mit
	REST},
  school = {Fernuniversität Hagen},
  year = {2010},
  type = {Diplomarbeit},
  month = {2},
  abstract = {Der Architekturstil REST wurde von Roy T. Fielding im Rahmen seiner
	Dissertation entworfen und im Jahr 2000 veröffentlicht. Hierbei handelt
	es sich um einen Stil für verteilte Systeme wie das World Wide Web.
	Der Fokus liegt bei REST auf den Ressourcen, das sind die Objekte
	einer Web Anwendung. Auch in jüngerer Zeit erfreut sich REST einer
	wachsenden Bedeutung. So gibt es inzwischen eine Vielzahl von Frameworks,
	die die Erstellung von Web Services nach den Prinzipien von REST
	unterstützen.
	
	
	Ziel dieser Arbeit war es, einige dieser Frameworks für die Programmiersprache
	Java zu untersuchen, zu vergleichen und zu beurteilen. Hierfür wurden
	folgende Frameworks festgelegt: die Java Servlet-API, die JAX-WS
	Referenz-Implementierung Metro, Jersey und RESTEasy als Implementierungen
	von JAX-RS, Restlet in den Versionen 1.1 und 2.0 sowie Spring-MVC.
	
	
	Zunächst wurde ein Fragenkatalog auf Basis der Dissertation von Roy
	Fielding abgeleitet. Dieser Katalog diente als Untersuchungsgrundlage
	für die ausgewählten Frameworks. Weiterhin wurde eine Beispielanwendung
	entworfen, welche anschließend mit jedem Framework implementiert
	wurde. Bei dem Entwurf stand die Abbildung verschiedenartiger Ressourcen
	im Vordergrund, um den jeweiligen Umgang in den Frameworks zu untersuchen.
	
	
	Als weiteres Werkzeug wurde ein Testclient auf Basis des Apache HTTP
	Clients entwickelt. Dadurch war es relativ einfach möglich, eine
	identische Abfolge von Requests an die implementierten Web Services
	zu schicken und die Antworten auszuwerten. Die so gewonnenen Ergebnisse
	wurden für die einzelnen Frameworks ausführlich dargestellt, um deren
	Stärken und Schwächen hervorzuheben. Anschließend wurden diese in
	einem Vergleich gegenübergestellt.},
  file = {:/home/thkoch/Desktop/studium/bachelor_thesis/references/rest/Diplomarbeit-schneider_vergleich_rest_frameworks.pdf:PDF},
  owner = {thkoch},
  timestamp = {2011.11.23},
  url = {http://www.fernuni-hagen.de/dvt/studium/themen_11.shtml}
}

